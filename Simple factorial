package factorial;
import java.net.*;
import java.util.*;
public class Sim_fact {
	public static void main(String args[]){ 
		Scanner scn= new Scanner(System.in);
		System.out.println("Enter your number: ");
		int num=scn.nextInt();
        int i=1,fact=1;
        while(i<=num)
        {
            fact=fact*i;
            i++;
        }
        System.out.println("Factorial of the number: "+fact);  
     }   
}
